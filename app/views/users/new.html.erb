<%= form_with model: @user, method: :post do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username %> <br>
  <%= form.label :email %>
  <%= form.text_field :email %> <br>
  <%= form.label :password %>
  <%= form.password_field :password %> <br>
  <%= form.label :password, "Password Confirmation" %>
  <%= form.password_field :password %> <br>
  <%= form.submit "Create User" %>
<% end %>
